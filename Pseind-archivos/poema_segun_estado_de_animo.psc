Proceso poema_segun_estado_de_animo
	Definir estadoAnimo Como Entero;
	Escribir 'Ingrese su estado de animo 1 (Feliz) | 2 (Desamor) | 3 (Enamorado) | 4 (Sad)';
	Leer estadoAnimo;
	
	Segun estadoAnimo hacer
		1:
			Escribir 'Que bueno';
		2:
			Escribir 'Xvr manito';
		3:
			Escribir 'Ella || El no te ama';
		4:
			Escribir 'Unas aguas';
		De Otro Modo:
			Escribir 'Hábil te crees';
			
		
	FinSegun
	
	
	
FinProceso
